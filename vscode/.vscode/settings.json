{
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "jinja",
    "javascriptreact",
    "typescriptreact",
    "markdown",
    "vue",
    "HTML (Eex)"
  ],
  "cSpell.language": "en,en-GB,en-US,ru",
  "cSpell.showStatus": false,
  "debug.allowBreakpointsEverywhere": true,
  "editor.accessibilitySupport": "off",
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.folding": false,
  "editor.fontFamily": "Monaco",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [
    80
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    "*.pyi": "python"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.idea": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/.coverage": true,
    "**/*.py[co]": true
  },
  "git.autofetch": false,
  "python.formatting.provider": "none",
  "python.pythonPath": "${workspaceFolder}/venv/bin/python3",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.enableBell": false,
  "vetur.validation.script": false,
  "vetur.validation.style": false,
  "vetur.validation.template": false,
  "window.clickThroughInactive": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "gitDecoration.modifiedResourceForeground": "#d4cb68",
    "terminal.background": "#272727",
    "terminal.foreground": "#ebdbb2",
    "terminal.ansiBlack": "#272727",
    "terminal.ansiBlue": "#448488",
    "terminal.ansiBrightBlack": "#928373",
    "terminal.ansiBrightBlue": "#83a597",
    "terminal.ansiBrightCyan": "#8ec07b",
    "terminal.ansiBrightGreen": "#b8ba25",
    "terminal.ansiBrightMagenta": "#d3859a",
    "terminal.ansiBrightRed": "#fb4833",
    "terminal.ansiBrightWhite": "#ebdbb2",
    "terminal.ansiBrightYellow": "#fabc23",
    "terminal.ansiCyan": "#689d69",
    "terminal.ansiGreen": "#989719",
    "terminal.ansiMagenta": "#b16185",
    "terminal.ansiRed": "#cc231c",
    "terminal.ansiWhite": "#a89983",
    "terminal.ansiYellow": "#d79920",
    "terminal.selectionBackground": "#ebdbb2",
    "terminal.integrated.rightClickCopyPaste": true,
    "terminal.integrated.enableBold": true
  },
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tips.enabled": false,
  "python.linting.enabled": true,
  "git.showInlineOpenFileAction": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "MesloLGSDZ Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.external.linuxExec": "gnome terminal",
  "python.jediEnabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.macOptionClickForcesSelection": true,
  "emmet.showSuggestionsAsSnippets": true,
  "javascript.suggestionActions.enabled": false,
  "workbench.colorTheme": "Gruvbox Dark (Medium)",
  "emmet.showSuggestionsAsSnipperts": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html",
    "razor": "html",
    "plaintext": "jade"
  }
}
