  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.showWelcomeMessage": false,
  "material-icon-theme.files.associations": {
    "firebase.utils.js": "firebase",
    "firebase.js": "firebase",
    "*.selectors.js": "Redux-store",
    "*.selectors.ts": "Redux-store",
    "*.slice.js": "Redux-reducer",
    "*.slice.ts": "Redux-reducer",
    "*.sagas.js": "Redux-store",
    "*.sagas.ts": "Redux-store",
    "*.saga.js": "Redux-store",
    "*.saga.ts": "Redux-store",
    "http.js": "http",
    "api.js": "http",
    "*.hook.js": "velocity",
    "http.ts": "http",
    "api.ts": "http",
    "*.hook.ts": "velocity"
  },
  "material-icon-theme.folders.associations": {
    "selectors": "redux-store",
    "router": "routes",
    "sagas": "Redux-store",
    "use-checker": "hook",
    "use-query": "hook",
    "use-sidebar": "hook",
    "use-toasts": "hook",
    "use-validation": "hook",
    "use-modal": "hook",
    "use-trash": "hook",
    "use-toggle": "hook",
    "use-sortable-data": "hook",
    "use-auth-user": "hook",
    "use-navigation-menu": "hook",
    "use-scroll-to-hash": "hook",
    "use-scroll-to-error": "hook",
    "use-form-handlers": "hook",
    "use-object-checker": "hook",
    "use-previous-value": "hook",
    "use-loading-effect": "hook",
    "private-route": "private",
    "public-route": "public",
    "filters": "review",
    "partials": "include"
  },
  // editor
  "eslint.validate": ["javascript", "typescript", "html"],

  "eslint.options": {
    "extensions": [".js", ".ts", "html"]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true,
    "source.fixAll.stylelint": true
    // "source.fixAll.prettier": true
  },
  "editor.accessibilitySupport": "off",
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.fontLigatures": true,
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.rulers": [100],
  "editor.scrollBeyondLastLine": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic (=FlottFlott)
          "comment",
          "entity.name.type.class", //class names
          "keyword", //import, export, return…
          // "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type",
          "constant.language",
          "entity.other.attribute-name",
          "variable.language"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics (VSCode has some defaults for italics)
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "variable.other"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.tabCompletion": "on",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "editor.tabSize": 2,
  "editor.showFoldingControls": "mouseover",
  "editor.folding": true,
  "editor.foldingStrategy": "indentation",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Operator Mono Lig, FiraCode Nerd Font",
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": false,
  // explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "explorer.sortOrder": "type",
  // files
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".stylelintrc": "jsonc",
    "*.pyi": "python",
    "*.html": "html",
    "*.styl": "stylus"
  },
  "files.trimFinalNewlines": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.vscode": false,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.idea": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/.coverage": true,
    "**/*.py[co]": true
  },
  // git
  "git.autofetch": false,
  "git.showInlineOpenFileAction": false,
  "git.confirmSync": false,
  // telemetry
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.enableBell": false,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.experimentalLinkProvider": false,
  "terminal.integrated.rendererType": "auto",
  // vetur
